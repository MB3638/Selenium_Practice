<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="suite">
    <test name="RegExpTest">
        <groups>
            <run>
                <include name="classic_.*"/>
                <!-- It will execute all the groups that starts with group name classic,
                     we use this regexp instead of including each group name individually-->
            </run>
        </groups>
        <classes>
            <class name="org.testng.RegularExpression">
            </class>
        </classes>
    </test>
</suite>